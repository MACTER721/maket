			color #fff
	&__button-description::after
		position absolute
		content ''
		top 0
		left 0
		bottom 0
		right 0
		transform translate(-13em, 0)
		width 0
		background #21a643
		z-index 3

	&__button-description:hover::after, 
	&__button-description:focus::after 
		animation sheen 1s forwards
		width 180px

	@keyframes sheen {
	  100% {
	    transform: translate(0em, 0);
	  }
	}
			

	&__button-select-apartment
		display flex
		justify-content center
		align-items center
		background-color #21a643
		font-size 12px
		color #fff
		width 200px
		height 50px
