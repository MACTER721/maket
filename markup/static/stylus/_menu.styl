.slider-navigation .slick-prev:before,
.slider-navigation .slick-next:before
	opacity 1 !important
	content '' !important

.slider-navigation .slick-prev
	top 0% !important
	left calc(50% - 24px) !important
	z-index 9
	background-repeat no-repeat
	background-image url('%=static=%/content/prev.png')
	background-position center
	background-color #fff
	height 45px
	width 41px
	
.slider-navigation .slick-next
	background-position center
	right calc(50% - 16px) !important
	top 420px
	background-repeat no-repeat
	background-image url(../img//content/next.png)
	height 43px
	width 40px
	background-color #fff

